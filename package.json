{
  "name": "@pragmatic-divops/remark-preset",
  "description": "remark preset for projects under the pragmatic-divops organization",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": "^14.18 || >=16"
  },
  "files": [
    "example.js",
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "runkitExampleFilename": "./example.js",
  "repository": "pragmatic-divops/remark-preset",
  "bugs": "https://github.com/pragmatic-divops/remark-preset/issues",
  "homepage": "https://npm.im/@pragmatic-divops/remark-preset",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "prepare": "husky install",
    "lint:engines": "ls-engines"
  },
  "devDependencies": {
    "@pragmatic-divops/commitlint-config": "1.0.24",
    "ban-sensitive-files": "1.9.18",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.1",
    "lockfile-lint": "4.9.5",
    "ls-engines": "0.7.0",
    "npm-run-all": "4.1.5",
    "remark-cli": "11.0.0"
  },
  "dependencies": {
    "@form8ion/remark-lint-preset": "4.0.2"
  }
}